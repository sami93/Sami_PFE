{
  "name": "angular2-full-stack",
  "version": "4.0.0",
  "license": "MIT",
  "author": "Davide Violante",
  "description": "Angular Full Stack project built using Angular 4, Express, Mongoose and Node.",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "sofrecom": "concurrently \"mongod\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "dev2": "concurrently \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "dev2c": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@ngtools/webpack": "^1.2.4",
    "angular2-jwt": "^0.2.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "bootstrap": "4.0.0-alpha.5",
    "chart.js": "^2.5.0",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "easy-pie-chart": "^2.1.7",
    "es6-promise-polyfill": "^1.2.0",
    "express": "^4.15.3",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "json2csv": "^3.11.0",
    "json2xls": "^0.1.2",
    "jsonexport": "^2.0.10",
    "jsonwebtoken": "^7.4.1",
    "jspdf": "^1.3.3",
    "mongodb": "^3.0.6",
    "mongoose": "^4.13.7",
    "mongoose-double": "0.0.1",
    "morgan": "^1.8.2",
    "ng-pdf-make": "0.0.2",
    "ng2-file-upload": "^1.2.1",
    "ng2-modal": "0.0.25",
    "node-rest-client": "^3.1.0",
    "node-xlsx": "^0.10.0",
    "rxjs": "^5.1.0",
